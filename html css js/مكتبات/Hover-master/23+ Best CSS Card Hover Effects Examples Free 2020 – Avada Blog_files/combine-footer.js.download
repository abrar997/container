function afterSearchComplete(){
  var secondSearch = $(".autocomplete-suggestions").eq(1);
  secondSearch.css({
    borderTop: '1px solid #e0e0e0',
    margin: '0 -1px',
    minWidth: 'auto'
  });
  currentWidth = secondSearch.width();
  newWidth = currentWidth + 5;
  secondSearch.css('width', newWidth);
}


